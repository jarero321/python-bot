# ===========================================
# Carlos Command - Environment Variables
# ===========================================
# Copia este archivo a .env y completa los valores

# App
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO

# Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_from_botfather
TELEGRAM_WEBHOOK_URL=https://your-ngrok-url.ngrok.io/webhook/telegram
TELEGRAM_CHAT_ID=your_chat_id

# Notion
NOTION_API_KEY=your_notion_integration_token

# Gemini
GEMINI_API_KEY=your_gemini_api_key

# Database
DATABASE_URL=sqlite+aiosqlite:///data/carlos_command.db

# Timezone
TZ=America/Mexico_City
